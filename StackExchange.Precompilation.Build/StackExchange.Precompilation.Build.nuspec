<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>StackExchange.Precompilation.Build</id>
    <version>$version$</version>
    <authors>SE inc</authors>
    <owners>SE inc</owners>
    <summary>StackExchange Precompilation Build Package</summary>
    <description>Replaces CSC and aspnet_compiler.exe with StackExchange.Precompiler for compiling C# (.cs) and Razor View (.cshtml) files in asp.net mvc 5 projects.</description>
    <tags>Razor</tags>
    <language>en-US</language>
  </metadata>
  <files>
      <file src="..\tools\*.*" target="tools" />
      <file src="*.targets" target="build" /><!-- .targets file name must match the package id -->
  </files>
</package>