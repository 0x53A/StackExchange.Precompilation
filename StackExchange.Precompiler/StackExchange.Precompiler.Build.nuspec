<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>StackExchange.Precompiler.Build</id>
    <version>$version$</version>
    <authors>SE inc</authors>
    <owners>SE inc</owners>
    <summary>StackExchange View Precompilation Package</summary>
    <description>msbuild package that replaces CSC and aspnet_compiler.exe with StackExchange.Precompiler for compiling C# (.cs) and Razor View (.cshtml) files in asp.net projects.</description>
    <tags>Razor</tags>
    <language>en-US</language>
  </metadata>
  <files>
      <file src="bin\Release\*.*" target="tools" />
      <file src="StackExchange.Precompiler.Build.targets" target="build\StackExchange.Precompiler.Build.targets" /><!-- .targets file name must match the package id -->
  </files>
</package>